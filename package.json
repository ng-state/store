    "domino": "^2.1.6",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "hammerjs": "^2.0.8",
    "http-server": "^14.1.0",
    "immer": "^10.1.1",
    "immutable": "^4.3.0",
    "immutable-cursor": "^2.0.1",
    "jest": "^29.5.0",
    "jest-preset-angular": "^14.2.2",
    "ng-packagr": "^18.2.1",
    "rxjs": "7.5.5",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
