    "@angular/platform-browser-dynamic": "^18.2.4",
    "@angular/platform-server": "^18.2.4",
    "@angular/router": "^18.2.4",
    "@angular/service-worker": "^18.2.4",
    "@ng-state/data-strategy": "^3.0.0",
    "@ng-state/immer-data-strategy": "^4.0.0",
    "@ng-state/immutablejs-data-strategy": "^3.0.0",
    "@ngneat/until-destroy": "9.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@types/es6-shim": "^0.31.42",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^27.5.1",
    "@types/node": "^12.20.36",
    "bootstrap": "^4.6.0",
    "core-js": "^3.22.7",
    "cpy-cli": "^4.1.0",
